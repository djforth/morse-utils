{
  "name": "@djforth/morse-utils",
  "version": "1.0.4",
  "description": "Utility Functions for Morse Site/App",
  "main": "index.js",
  "scripts": {
    "build": "babel src -x '.js'  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "test": "npm run lint && asset-pipeline-karma",
    "test:watch": "asset-pipeline-karma -w",
    "test:mac": "asset-pipeline-karma -b"
  },
  "assets":{
    "assets_in": "./src",
    "assets_out": "./lib",
    "karma":{
      "input":"./spec"
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/morse-utils.git"
  },
  "keywords": [
    "utility",
    "functions",
    "sites",
    "app"
  ],
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/morse-utils/issues"
  },
  "homepage": "https://github.com/djforth/morse-utils#readme",
  "devDependencies": {
    "@djforth/ap_karma": "1.0.0",
    "@djforth/cookie_mgmt_fp": "^1.0.2",
    "@djforth/morse-jasmine": "^1.0.8",
    "babel": "^6.5.2",
    "babel-cli": "^6.6.0",
    "babel-eslint": "^5.0.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.2.0",
    "eslint-config-google": "^0.4.0",
    "lodash": "^4.6.1"
  },
  "peerDependencies": {
    "@djforth/cookie_mgmt_fp": "^1.0.2",
    "lodash": "^4.6.1"
  }
}
