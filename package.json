{
  "name": "@djforth/morse-utils",
  "version": "1.2.4",
  "description": "Utility Functions for Morse Site/App",
  "main": "index.js",
  "scripts": {
    "build": "babel src -x '.js'  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "test": "./node_modules/karma/bin/karma start --browsers PhantomJS --single-run",
    "test:mac": "./node_modules/karma/bin/karma start --browsers Chrome,Firefox,Safari  --single-run",
    "test:watch": "node --max_old_space_size=4096 ./node_modules/karma/bin/karma start --browsers PhantomJS"
  },
  "assets": {
    "assets_in": "./src",
    "assets_out": "./lib",
    "karma": {
      "input": "./spec"
    }
  },
  "babel": {
    "plugins": [
      "react-hot-loader/babel"
    ],
    "presets": [
      "es2015",
      "es2016",
      "es2017",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "morsedigital"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/morse-utils.git"
  },
  "keywords": [
    "utility",
    "functions",
    "sites",
    "app"
  ],
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/morse-utils/issues"
  },
  "homepage": "https://github.com/djforth/morse-utils#readme",
  "devDependencies": {
    "@djforth/cookie_mgmt_fp": "^1.0.2",
    "@djforth/morse-jasmine-wp": "^2.4.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "2.0.1",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-rewire-ignore-coverage": "^1.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "eslint": "^3.7.1",
    "eslint-config-google": "^0.6.0",
    "eslint-config-morsedigital": "^1.0.3",
    "eslint-plugin-jasmine": "^1.8.1",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-story-reporter": "^0.3.1",
    "karma-webpack": "^1.8.0",
    "lodash": "~4.16.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "peerDependencies": {
    "@djforth/cookie_mgmt_fp": "^1.0.2",
    "lodash": "^4.*.*"
  }
}
